{
  "manifest_version": 2,

  "name": "iTrace-Chrome Plguin",
  "description": "Plugin which allows for tracking eyegaze data on webpages in Chrome.",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "unlimitedStorage",
    "system.display"
  ]
}
